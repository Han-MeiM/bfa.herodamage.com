    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-remark": "^2.1.5",
    "jss": "^9.8.7",
    "little-loader": "^0.2.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-google-charts": "^3.0.8",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1",
    "react-timeago": "^4.1.9",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
